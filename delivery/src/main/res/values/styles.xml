<resources>

    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!-- API 14 theme customizations can go here. -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@color/color_white</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <style name="Text_333_34">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="ListView_common_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:scrollbars">none</item>
        <item name="android:cacheColorHint">@color/color_transparent</item>
        <item name="android:listSelector">@color/color_transparent</item>
        <item name="android:dividerHeight">2dp</item>
        <item name="android:divider">@color/color_transparent</item>
    </style>

</resources>
